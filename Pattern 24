#include<iostream>
using namespace std;

int main(int argc, char const *argv[])
{
	int rows = 7;
	int cols = rows;
	
	for(int i=1;i<=rows;i++){
		for (int j = 1; j <=cols; ++j)
		{
			if((i==1 || i==7 || j==1 || j==7) || ((i>=3 && i<=5 && j>=3 && j<=5) && (i==3 || i==5 || j==3 || j==5))){
				cout << "*";
			}else{
				cout << " ";
			}
		}
		cout<< "\n";
	}
	return 0;
}
